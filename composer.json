{
  "name": "cisse/as400-bundle",
  "description": "Symfony bundle for IBM AS400/DB2 database connections",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Symfony Community",
      "homepage": "https://symfony.com/contributors"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-pdo": "*",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/config": "^6.4|^7.0",
    "symfony/console": "^6.4|^7.0",
    "symfony/web-profiler-bundle": "^6.4|^7.0",
    "psr/log": "^1.0|^2.0|^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "symfony/phpunit-bridge": "^6.4|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Cisse\\Bundle\\As400\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cisse\\Bundle\\As400\\Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
